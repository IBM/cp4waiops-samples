    {
      "perspective": "dashboard",
      "features": [
        {
          "id": "com.ibm.dashboard.aiops.topo.widget",
          "collectionItems": [
            {
              "containerId": "com.ibm.bi.dashboard.contentTypes",
              "id": "widget.topoWidget.renderer",
              "type": "topologyViewerWidget",
              "iconUrl": "v1/ext/AIOpsCustomWidgets/assets/network.svg",
              "expose": true,
              "name": "AIOps Topology Viewer",
              "propertyLayoutList": [
                {
                  "type": "Section",
                  "id": "aiops_settings",
                  "label": "AIOPs",
                  "position": 0
                }
              ],
              "propertyList": [
                {
                  "id": "toggleToolbar",
                  "editor": {
                    "sectionId": "general.aiops_settings",
                    "uiControl": {
                      "label": "Show toolbar",
                      "type": "ToggleButton",
                      "ariaLabel": "Show toolbar",
                      "value": true
                    }
                  }
                },
                {
                  "id": "toggleSearch",
                  "editor": {
                    "sectionId": "general.aiops_settings",
                    "uiControl": {
                      "label": "Show search",
                      "type": "ToggleButton",
                      "ariaLabel": "Show search",
                      "value": true
                    }
                  }
                },
                {
                  "id": "hops",
                  "editor": {
                    "sectionId": "general.aiops_settings",
                    "uiControl": {
                    "type": "InputLabel",
                    "label": "Hops"
                    }
                  }
                },
                {
                  "id": "maxResourceIds",
                  "editor": {
                    "sectionId": "general.aiops_settings",
                    "uiControl": {
                    "type": "InputLabel",
                    "label": "Max unique resource IDs",
                    "value": 10
                    }
                  }
                },
                {
                  "id": "resourceId",
                  "editor": {
                    "sectionId": "general.aiops_settings",
                    "uiControl": {
                    "type": "InputLabel",
                    "label": "Resource ID"
                    }
                  }
                },
                {
                  "id": "statusId",
                  "editor": {
                    "sectionId": "general.aiops_settings",
                    "uiControl": {
                    "type": "InputLabel",
                    "label": "Status ID"
                    }
                  }
                }
              ],
              "renderer": {
                "class": "v1/ext/AIOpsCustomWidgets/topology-viewer/TopologyViewerWidget",
